{
  "manifest_version": 3,
  "name": "HTML Class Content Analyzer",
  "version": "1.0",
  "description": "Analyzes and exports HTML class content from webpages",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
} 